@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<br />

<table id="gridDadosAnamnese" class="table table-condensed table-hover table-striped">
    <thead>
        <tr>
            <th data-column-id="ModeloId" data-identifier="true" data-type="numeric">Código</th>
            <th data-column-id="Nome" data-order="asc">Nome do Modelo</th>
            <th data-column-id="commands" data-formatter="commands" data-sortable="false">Ação</th>
        </tr>
    </thead>
</table>

<center>
    <p>
        @Html.ActionLink("Cadastrar Novo Modelo/Pergunta", "Novo")
        
      
</center>


@section Scripts {
    @Scripts.Render("~/Scripts/Telas/Anamnese/index.js")

}




