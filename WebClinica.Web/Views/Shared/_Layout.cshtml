                                        <li><a href="@Url.Action("Index","Profissional")">Profissional de Saúde</a> </li>
                                    </ul>
                                </li>

                                <li class="dropdown">
                                    <a href="#" class="dropdown-toggle" data-toggle="dropdown">Cadastros<b class="caret"></b></a>
                                    <ul class="dropdown-menu">
                                        <li><a href="#">Agendamento de Consulta</a> </li>
                                        <li><a href="@Url.Action("Index","Fornecedor")">Fornecedor</a> </li>
                                        <li class="divider"></li>
                                        <li><a href="#">Solicitação de Exame</a> </li>
                                        <li><a href="@Url.Action("Index","Usuario")">Usuário</a> </li>
                                        <li><a href="@Url.Action("Index","TipodeConsulta")">Tipo de Consulta</a> </li>

                                    </ul>
                                </li>
                                <!-- Menu dropdown Fim -->

                                <li><a href="#">Estoque <span class="sr-only">(current)></span></a></li>

                                <li class="dropdown">
                                    <a href="#" class="dropdown-toggle" data-toggle="dropdown">Financeiro<b class="caret"></b></a>
                                    <ul class="dropdown-menu">
                                        <li><a href="#">Banco</a> </li>
                                        <li><a href="#">Conta</a> </li>
                                        <li><a href="#">Comissão</a> </li>
                                        <li><a href="#">Faturas</a> </li>
                                        <li class="divider"></li>
                                        <li><a href="#">Estorno</a> </li>
                                        <li><a href="#">Movimentação</a> </li>
                                        <li><a href="#">Orçamento</a> </li>
                                        <li><a href="#">Títulos</a> </li>
                                        <li class="divider"></li>
                                        <li><a href="#">Painel Financeiro</a> </li>
