                            <a class="navbar-brand" href="#" target="_blank">WebClinica</a>

                        </div>
                        <!-- Menu hamburger Fim -->

                        <div class="collapse navbar-collapse" id="exemplo-navbar-collapse">

                            <!-- Links Inicio -->
                            <ul class="nav navbar-nav navbar-left">
                                <!-- Menu dropdown Inicio -->
                                <li><a href="">Agenda <span class="sr-only">(current)></span></a></li>                               
