                                        <li><a href="@Url.Action("Index","Paciente")">Manter Paciente</a> </li>
                                    </ul>
                                </li>

                                <li><a href="@Url.Action("Index","ConfiguracaoClinica")">Parâmetros do Sistema <span class="sr-only">(current)></span></a></li>


