@{
    ViewBag.Title = "Tabelas";

}
<br />

<table id="gridDadosTabelasDerivadas" class="table table-condensed table-hover table-striped">
    <thead>
        <tr>
            <th data-column-id="TabelaDerivadaCobrancaProcedimentoID" data-identifier="true" data-type="numeric">Código</th>
            <th data-column-id="NomeTabela" data-order="asc">Tabela</th>            
            <th data-column-id="commands" data-formatter="commands" data-sortable="false">Ação</th>
        </tr>
    </thead>
</table>

<center>
    <p>
        @Html.ActionLink("Cadastrar Nova Tabela", "Novo")
</center>


@section Scripts {
    @Scripts.Render("~/Scripts/Telas/TabelaDerivada/index.js")
}


