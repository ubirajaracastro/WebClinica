
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<br /><br />

<table id="gridDadosProcedimento" class="table table-condensed table-hover table-striped">
    <thead>
        <tr>
            <th data-column-id="ProcedimentoId" data-identifier="true" data-type="numeric">Código</th>
            <th data-column-id="Nome" data-order="asc">Nome</th>
            <th data-column-id="Descricao" data-order="asc">Descrição do Procedimento</th>
            <th data-column-id="Ativo" data-order="desc">Ativo</th>           
            <th data-column-id="commands" data-formatter="commands" data-sortable="false">Ação</th>
        </tr>
    </thead>
</table>

<center>
    <p>
        @Html.ActionLink("Cadastrar Novo Procedimento", "Novo")
</center>


@section Scripts {
    @Scripts.Render("~/Scripts/Telas/Procedimento/index.js")
}
