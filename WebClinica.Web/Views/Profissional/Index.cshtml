@{
    ViewBag.Title = "Profissionais Cadastrados";
    Layout = "~/Views/Shared/_Layout.cshtml";

}
<br /><br />

<table id="gridDadosProfissional" class="table table-condensed table-hover table-striped">
    <thead>
        <tr>
            <th data-column-id="ProfissionalId" data-identifier="true" data-type="numeric">Código</th>
            <th data-column-id="Nome" data-order="asc">Profissional</th>
            <th data-column-id="Ativo" data-order="desc">Ativo</th>
            @*<th data-column-id="link" data-formatter="link" data-sortable="false">Link</th>*@
            <th data-column-id="commands" data-formatter="commands" data-sortable="false">Ação</th>
        </tr>
    </thead>
</table>

<center>
    <p>
        @Html.ActionLink("Cadastrar Novo Profissional", "Novo")
</center>


@section Scripts {
    @Scripts.Render("~/Scripts/Telas/ProfissionalIndex.js")
}


